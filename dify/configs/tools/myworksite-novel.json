{
    "openapi": "3.1.0",
    "info": {
      "title": "myworksite-novel",
      "description": "myworksite server, novel part",
      "version": "v1.0.0"
    },
    "servers": [
      {
        "url": "http://host.docker.internal:23001"
      }
    ],
    "paths": {
      "/api/aiNoval/geo/findOne": {
        "get": {
          "description": "Get one geography unit by name",
          "operationId": "GetGeoOneByName",
          "parameters": [
            {
              "name": "worldview_id",
              "in": "query",
              "description": "Id of worldview,",
              "required": false,
              "schema": {
                "oneOf": [
                  {
                    "const": "1",
                    "title": "aeris solar system",
                    "description": "worldview of 'aeris solar system'"
                  }
                ]
              }
            },
            {
              "name": "name",
              "type": "string",
              "in": "query",
              "description": "name of geography object"
            }
          ]
        },
        "deprecated": false
      },
      "/api/aiNoval/faction/list": {
        "get": {
          "description": "Get all factions for a specific worldview",
          "operationId": "GetFactionListOfWorldView",
          "parameters": [
            {
              "name": "worldview_id",
              "in": "query",
              "description": "Id of worldview",
              "required": false,
              "schema": {
                "oneOf": [
                  {
                    "const": "1",
                    "title": "aeris solar system",
                    "description": "worldview of 'aeris solar system'"
                  }
                ]
              }
            },
            {
              "name": "name",
              "type": "string",
              "in": "query",
              "description": "name of faction"
            },
            {
              "name": "page",
              "type": "number",
              "in": "query",
              "description": "page number, default 1" 
            },
            {
              "name": "limit",
              "type": "number",
              "in": "query",
              "description": "limit of page data, default 200" 
            }
          ]
        }
      },
      "/api/aiNoval/role/info/list": {
        "get": {
          "description": "Get all characters for a specific worldview",
          "operationId": "GetCharacterListOfWorldView",
          "parameters": [
            {
              "name": "worldview_id",
              "in": "query",
              "description": "Id of worldview",
              "required": false,
              "schema": {
                "oneOf": [
                  {
                    "const": "1",
                    "title": "aeris solar system",
                    "description": "worldview of 'aeris solar system'"
                  }
                ]
              }
            },
            {
              "name": "name",
              "type": "string",
              "in": "query",
              "description": "name of character"
            },
            {
              "name": "page",
              "type": "number",
              "in": "query",
              "description": "page number, default 1" 
            },
            {
              "name": "limit",
              "type": "number",
              "in": "query",
              "description": "limit of page data, default 200" 
            }
          ]
        }
      }
    },
    "components": {
      "schemas": {}
    }
  }